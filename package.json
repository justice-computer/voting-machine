{
	"name": "chat-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"fix": "concurrently \"bun:fix:*\"",
		"fix:biome:format": "biome format --write .",
		"fix:biome:check": "biome check --write *",
		"fix:eslint": "eslint --fix .",
		"lint": "concurrently \"bun:lint:*\"",
		"lint:biome:check": "biome check",
		"lint:biome:format": "biome format .",
		"lint:eslint": "eslint .",
		"lint:types": "tsc --noEmit",
		"run": "serve",
		"preview": "vite preview",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"nuke": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm pnpm-lock.yaml"
	},
	"dependencies": {
		"concurrently": "8.2.2",
		"eslint-plugin-import": "2.29.1",
		"firebase": "10.12.4",
		"framer-motion": "11.3.8",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-toastify": "10.0.5",
		"serve": "14.2.3",
		"zustand": "4.5.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "2.27.7",
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.7.0",
		"@faker-js/faker": "8.4.1",
		"@testing-library/jest-dom": "6.4.6",
		"@testing-library/react": "16.0.0",
		"@tsconfig/vite-react": "3.0.2",
		"@types/node": "20.14.11",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "7.17.0",
		"@typescript-eslint/parser": "7.17.0",
		"@vitejs/plugin-react": "4.3.1",
		"atom.io": "0.25.4",
		"c8": "10.1.2",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"jsdom": "24.1.1",
		"sass": "1.77.8",
		"typescript": "5.5.3",
		"vite": "5.3.4",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "2.0.4"
	},
	"packageManager": "pnpm@9.6.0"
}

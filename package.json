		"react-toastify": "10.0.5",
		"serve": "14.2.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@changesets/cli": "2.27.9",
		"@chromatic-com/storybook": "2.0.2",
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.11.1",
		"@faker-js/faker": "9.0.3",

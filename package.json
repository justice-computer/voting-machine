{
	"name": "chat-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"fix": "concurrently \"bun:fix:*\"",
		"fix:biome:format": "biome format --write .",
		"fix:biome:check": "biome check --write *",
		"fix:eslint": "eslint --fix .",
		"lint": "concurrently \"bun:lint:*\"",
		"lint:biome:check": "biome check",
		"lint:biome:format": "biome format .",
		"lint:eslint": "eslint .",
		"lint:types": "tsc --noEmit",
		"run": "serve",
		"preview": "vite preview",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"nuke": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm pnpm-lock.yaml",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"concurrently": "8.2.2",
		"eslint-plugin-import": "2.29.1",
		"firebase": "10.13.0",
		"framer-motion": "11.3.29",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-toastify": "10.0.5",
		"serve": "14.2.3",
		"zustand": "4.5.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "2.27.7",
		"@chromatic-com/storybook": "1.7.0",
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.9.0",
		"@faker-js/faker": "8.4.1",
		"@storybook/addon-essentials": "8.2.9",
		"@storybook/addon-interactions": "8.2.9",
		"@storybook/addon-links": "8.2.9",
		"@storybook/addon-onboarding": "8.2.9",
		"@storybook/blocks": "8.2.9",
		"@storybook/react": "8.2.9",
		"@storybook/react-vite": "8.2.9",
		"@storybook/test": "8.2.9",
		"@testing-library/jest-dom": "6.4.8",
		"@testing-library/react": "16.0.0",
		"@tsconfig/vite-react": "3.0.2",
		"@types/eslint": "9.6.0",
		"@types/node": "20.16.1",
		"@types/react": "18.3.4",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "8.2.0",
		"@typescript-eslint/parser": "8.2.0",
		"@vitejs/plugin-react": "4.3.1",
		"atom.io": "0.28.1",
		"c8": "10.1.2",
		"eslint": "9.9.0",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"jsdom": "24.1.1",
		"sass": "1.77.8",
		"storybook": "8.2.9",
		"typescript": "5.5.4",
		"vite": "5.4.2",
		"vite-tsconfig-paths": "5.0.1",
		"vitest": "2.0.5"
	},
	"packageManager": "pnpm@9.8.0"
}
